<body >
  <% include ../partials/navbar.ejs %>   

  <div class="container mobile-container" >
  <!--  Blog Title-->
  <div class="text-center" style="margin-top:15px ;margin-bottom:15px">
    <h1 class="text-center title-div" style="color:black; font-weight: bold; padding-top:15px; padding-bottom:15px;" ><%= blog.title %></h1>
    <!--  Blog DAte && Blog Creator-->
    <h5 class="text-muted" ><%= blog.time %> &nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;Posted By: <%= blog.creator %></h5>
  
    
  </div>
    <!---- Blog Image-->
    <% if(blog.image.localeCompare("")==1){%>
    <div class="text-center" style="margin-top:20px; margin-bottom:20px">
    <img style="width:100%; height:auto; object-fit: contain;" src="/uploads/<%= blog.image%>" alt="blog-image">
    </div>
    <%  }%>
    
<!-- 

1. load font awesome with your favorite method
2. replace items in {{ }}s with details specific to your content 

-->

<div id="share">

  <!-- facebook -->
  <a class="facebook" href="https://www.facebook.com/share.php?u=https://bailabollywood.com/blogs/posts/<%=blog._id%>&title=<%=blog.title%>" target="blank"><i class="fa fa-facebook"></i></a>

  <!-- twitter -->
  <a class="twitter" href="https://twitter.com/intent/tweet?text=<%=blog.title%>%20+https://bailabollywood.com/blogs/posts/<%=blog._id%>" target="blank"><i class="fa fa-twitter"></i></a>

<a class="whatsapp" href="whatsapp://send?text=<%=blog.title%>%20+https://bailabollywood.com/blogs/posts/<%=blog._id%>" data-action="share/whatsapp/share" target="blank"><i class="fa fa-whatsapp"></i></a>

 
	
  <!-- google plus -->
  <a class="googleplus" href="https://plus.google.com/share?url=https://bailabollywood.com/blogs/posts/<%=blog._id%>" target="blank"><i class="fa fa-google-plus"></i></a>

  <!-- linkedin -->
  <a class="linkedin" href="https://www.linkedin.com/shareArticle?mini=true&url=https://bailabollywood.com/blogs/posts/<%=blog._id%>&title=<%=blog.title%>&source=bailabollywood.com" target="blank"><i class="fa fa-linkedin"></i></a>
  
  <!-- pinterest 
  <a class="pinterest" href="https://pinterest.com/pin/create/bookmarklet/?media=<%=blog.thumbnail%>&url=https://bailabollywood.com/blogs/posts/<%=blog._id%>&is_video=false&description=<%=blog.title%>" target="blank"><i class="fa fa-pinterest-p"></i></a>
  -->
</div>

    
    <div class="container"  >
    <!---  Blog Content -->
      <p class="target" style="text-align: justify !important; font-size: 18px;"><%= blog.content %></p>
    </div>
    
    <div class="container">
        <%subBlogz.forEach(function(subBlog){%>
          
          <div>
          <h4 style="font-weight: 1000; margin-top:20px;"><%=subBlog.title%></h4>
          </div>
         <% if(subBlog.image.localeCompare("")==1){%>
          <div  class="text-center">
            <img style="width:100%; height:auto; object-fit: contain; margin-top: 20px; margin-bottom: 20px;" src="<%=subBlog.image%>" alt="image">
          </div>
        <%}%>
          <div class="target mt-2" style="text-align: justify !important; font-size: 18px;"><%=subBlog.content%></div>
        
        <%})%>
      </div>
      <hr>
     <% if(prevId!=0){%>
      <p>
        <a  href='/blogs/posts/<%=prevId%>'>
          <input type="button"  value="<-<%=prev[0].title%>" class="float-left">
          </a>
        </p>

      <%}if(nextId!=0){%>
        <br><br>
        <p style="padding-top: 5px;">
        <a href="/blogs/posts/<%=nextId%>">
          <input type="button"  value="<%=next[0].title%>->" class="float-right">
        </a>
      </p>
      <br>
      <%}%>
      
      <div class="container text-center" style="margin-top: 50px;" >
        
        <h2>Te puede interesar</h2>
        <div id="lt-blog" class="mt-10 mb-5">
          <a href="/blogs/posts/<%=suggestionBlog[0]._id%>">
            
            <% if(suggestionBlog[0].image.localeCompare("")==1){%>
                    
              <img alt="image" class="suggestedBlog"  src="/uploads/<%= suggestionBlog[0].thumbnail %>" />
              <%} else {%>
                
                
                <img alt="image" class="suggestedBlog"  src="<%= suggestionBlog[0].thumbnail %>"/>
                <%}%>
       
       
          </a>
        <p style="color:black;font-weight:bolder; font-size: 20px;"><%=suggestionBlog[0].title%><br><span style="color: black; font-size: 17px;">Categoría-<%=suggestionBlog[0].tag%></span></p>
      
    </div>
      </div>
      
 <% include ../partials/footer-blog.ejs %>   
 
    
  
  
  <div class="mobile-container text-center">
    <hr>
    <div style="padding-top: 100px; padding-bottom: 50px;">
    <form action="/blogs/posts/<%= blog._id %>" method="POST">

      <h3  style="color:grey">Leave A Reply </h3>
      <input required type="text" style="width: 100%; margin-top:5px; margin-bottom: 15px;" name="comment[name]" placeholder="Name">
      <input required type="email" style="width: 100%;  margin-bottom: 15px;" name="comment[email]" placeholder="Email">
      <textarea required style="width: 100%;" name="comment[text]" rows="5" placeholder="Add Comment Here"></textarea>
      <input required style="margin-top: 20px;  width:150px; "  type="submit" value="Post Comment" />
    </form>
  </div>
  </div> 

  <div class="mobile-container" style="margin-bottom:100px ">
    <% commentz.forEach(comment=>{%>
      <div class="media" style="background-color: #f5f5f5; border-radius: 10px; margin-top: 20px;">
        <div class="media-body">
          <h5 style="font-weight: bold; padding-left: 10px; padding-top: 10px;"><%=comment.name%></h5>
         <p style="padding-top:5px; padding-left: 10px;"> <%= comment.text %></p>
        </div>
      </div>
    <%})%>
  </div>

  

 
 

  


<!--Parsing HTML JS-->
<script>
$(".target").each(function(){
    let text = ( $(this).text());

    let html = $.parseHTML(text);
    $(this).empty();
    $(this).append(html);
  });
</script>
