    <!-------------------------------Interview Remaining Part------------------------->
        
        <% Interview.subInterviews.forEach(function(data){if(data.title!=("")){%>
          
            <div >
                <h5><strong><%=data.title%></strong></h5>
            </div>
        <%}if(data.image!=("")){%>
            <div class="text-center" >
                <img style="max-width:100%; height:auto;" class="mt-2 mb-2" src="<%=data.image%>" alt="interview-image">
            </div>
        <%}if(data.content!=("")){%>
            <div>
                <div class="target" style="text-align: justify !important;"><%=data.content%></div>
            </div>
        <%}})%>
      


