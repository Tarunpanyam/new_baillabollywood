 }
.container-fluid {
    width: 80%;
    margin-top: 50px;
}
.col-12.col-sm-12.col-md-5, .col-12.col-sm-12.col-md-7 {
    padding-left: 0px;
    padding-right: 0px;
    height: 100%;
}
.image-wrapper img {
    width: 100%;

}
#lt-blog p {
    padding-top: 10px;
} 
#read-btn {
    background: transparent;
    font-size: 20px;
    padding: 10px 40px;
    border: 2px solid black;
    color: black;
    border-radius: 20px;
    background:linear-gradient(to right,#ff5f6d,#f7b733)
}
  
#read-btn:hover {
    border-color: white;
    color: #fff;
}

div#lt-blog p span {
    color: #a9a9a9;
}
div#lt-blog img {
    border: 2px solid #ffffff;
    padding: 10px;
}
div#lt-blog a .forImgHover:hover {
    transform: scale(1.1) !important;
}
.hover-wrapper:hover{
  transform: scale(1.1) !important;

}
.container p {font-size:20px; }

