  <!------------------------------Instrument Start ------------------------>
  <div class="container mobile-container">

    <div class="text-center" style="margin-top:15px ;margin-bottom:15px">
        <h1 class="text-center title-div" style="color:black; font-weight: bold; padding-top:15px; padding-bottom:15px;" ><%= instrument.title %></h1>
        <!--  Blog DAte && Blog Creator-->
      
        
      </div>


      <div class="text-center" style="margin-top:20px; margin-bottom:20px">
        <%if(instrument.image!==""){%>
        <img style="width:100%; height:auto; object-fit: contain;" src="<%= instrument.image%>" alt="instrument-image">
    <%}%>
            
          
    </div>
        <div class="container"  >

            <!---  Blog Content -->
              <p class="target" style="text-align: justify !important; font-size: 18px;"><%= instrument.content %></p>
              <a href="/instruments/posts/<%=instrument._id%>/edit" target="_blank">
                <button style="width: 80%;" class="btn btn-primary">Edit This Part</button>
              </a>
              <a href="/instruments/posts/<%=instrument._id%>/delete">
                <button style="width: 80%;"  class="btn btn-danger">Delete This Part</button>
              </a>
            </div>
      

  </div>
  <!-------------------------------Instrument End ------------------------->
  <!---------------------------------Subinstrument start-->

  <div class="mobile-container">
    <%instrument.subInstruments.forEach(function(subInstrument){%>
      
      <div>
      <h4 style="font-weight: 1000; margin-top:20px;"><%=subInstrument.title%></h4>
      </div>
     <% if(subInstrument.image!==""){%>
      <div  class="text-center">
        <img style="width:100%; height:auto; object-fit: contain; margin-top: 20px; margin-bottom: 20px;" src="<%=subInstrument.image%>" alt="image">
      </div>
    <%}%>
      <div class="target mt-2" style="text-align: justify !important; font-size: 18px;"><%=subInstrument.content%></div>
      <div style="margin-top:20px;">
        <a href="/instruments/posts/<%=instrument._id%>/subInstruments/<%=subInstrument._id%>/edit" target="_blank">
        <button style="width: 80%;" class="btn btn-primary">Edit This Part</button>
      </a>
      <a href="/instruments/posts/<%=instrument._id%>/subInstruments/<%=subInstrument._id%>/delete">
        <button style="width: 80%;"  class="btn btn-danger">Delete This Part</button>
      </a>
      </div>
    <%})%>
  </div>
