<body >
<h1 class="text-center"> Admin Individual Page for Bollywood News</h1>
  
      <!----------------------------BollywoodNews Main Part --------------------->
    <div class="container-fluid">
        <div class="container-sm">
            <div class="text-center" >
                <img style="max-width:100%; max-height:auto;" class="mt-5 mb-1" src="<%=BollywoodNews.image%>" alt="news-image">
            </div>
        <div style="margin-top:12px;">
            <h2><i><%=BollywoodNews.title%></i></h2>
            <div style="border-top:2px solid #bfbfbf">
            <p ><small><strong>May 13 2020&emsp;|&emsp;By <%=BollywoodNews.uploader%></strong></small></p>
        </div>
        </div>
        <div>
            <p class="target"><%=BollywoodNews.content%></p>
        </div>
        <div class="container">
          <div class="text-center" style="margin-top:20px; margin-bottom: 20px;">
            <a href="/canal-bollywood/posts/<%=BollywoodNews._id%>/add-more-information">
            <button style="width: 80%; background-color: black;" class="btn btn-primary">Add More Information To This BollywoodNews</button>
          </a>
          </div>
            <div class="text-center" style="margin-top:20px; margin-bottom: 20px;">
              <a href="/canal-bollywood/posts/<%=BollywoodNews._id%>/edit">
              <button style="width: 80%; background-color: black;" class="btn btn-primary">Edit This Part</button>
            </a>
            </div>
            <div class="text-center" style="margin-top:20px; margin-bottom: 20px;">
              <a href="/canal-bollywood/posts/<%=BollywoodNews._id%>">
              <button style="width: 80%; background-color: black;" class="btn btn-primary">View BollywoodNews</button>
            </a>
            </div>
          </div>
        
        
    
        
        
        
    <!---------------------------------------------------------------------------->


    <!-------------------------------BollywoodNews Remaining Part------------------------->
    
        <% BollywoodNews.subNews.forEach(function(data){if(data.title!=("")){%>
            <div class="mt-5 mb-5">
                <h5><strong><%=data.title%></strong></h5>
            </div>
        <%}if(data.image!=("")){%>
            <div class="text-center" >
                <img style="max-width:100%; height:auto;" class="mt-5 mb-1" src="<%=data.image%>" alt="news-image">
            </div>
        <%}if(data.content!=("")){%>
            <div>
                <p class="target"><%=data.content%></p>
            </div>
            <%}%>
      <!-------------------------------------Edit Button For Part---------------------->
      <div class="container">
        <div style="margin-top:20px;">
          <a href="/canal-bollywood/posts/<%=BollywoodNews._id%>/subNews/<%=data._id%>/edit">
          <button style=" background-color: black; width: 20%;" class="btn btn-primary">Edit This Part</button>
        </a>
        <a href="/canal-bollywood/posts/<%=BollywoodNews._id%>/subNews/<%=data._id%>/delete">
          <button style=" background-color: black; width: 25%;" class="btn btn-primary">Delete This Part</button>
        </a>
        </div>
      </div>
        <%})%>

        


    </div>
    
    
    


</div>

  






